@model kits.Models.order

@{
    ViewBag.Title = "Delete";
}




<div class="deletview">
    <div class="head">
        <h2>Delete order</h2>
        <h3>Are you sure you want to delete this?</h3>
    </div>
    <div class="deleteText">
        <p>
            @Html.DisplayNameFor(model => model.state.name):   @Html.DisplayFor(model => model.state.name)
            <br /> <br />
            @Html.DisplayNameFor(model => model.user.user_email): @Html.DisplayFor(model => model.user.user_email)
        </p>
    </div>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

    <div class="form-actions no-color detailsbuttonview">
        <input type="submit" value="Delete" class="btn btn-default edidtButtom delete " />
        <input type="button" class="edidtButtom" value="Back to list" onclick="location.href='@Url.Action("index")'" />
      
    </div>
    }
</div>
